            HEADER,AT(,,5990,677)
              STRING(@P###-##-####P),AT(3406,63),USE(STU:Number),FONT(,,COLOR:Black,FONT:bold), |
                RIGHT(1),#ORIG(STU:Number)
              STRING('Room'),AT(2875,292),USE(?String9),FONT(,,COLOR:Black,FONT:bold),#ORIG(?String9)
              STRING('Time'),AT(3688,292),USE(?String10),FONT(,,COLOR:Black,FONT:bold),#ORIG(?String10)
              LINE,AT(50,560,5900,0),USE(?Line2),COLOR(COLOR:Black),#ORIG(?Line2)
              STRING('Class'),AT(479,292),USE(?String11),FONT(,,COLOR:Black,FONT:bold),#ORIG(?String11)
              STRING(@S20),AT(83,63),USE(STU:FirstName),FONT(,,COLOR:Black,FONT:bold),#ORIG(STU:FirstName)
              STRING(@S20),AT(1760,63),USE(STU:LastName),FONT(,,COLOR:Black,FONT:bold),#ORIG(STU:LastName)
            END
detail      DETAIL,AT(-10,10,6000,302)
              STRING(@S30),AT(250,42),USE(COU:Description),#ORIG(COU:Description)
              STRING(@n4),AT(2875,42),USE(CLA:RoomNumber),#ORIG(CLA:RoomNumber)
              STRING(@s20),AT(3688,42),USE(CLA:ScheduledTime),#ORIG(CLA:ScheduledTime)
            END
            FOOTER,AT(,,,323),PAGEAFTER(-1)
            END
          END
          FOOTER,AT(1000,9000,6000,219)
            STRING(@pPage <<<#p),AT(5250,30,700,135),USE(?PageCount),FONT('Arial',8,COLOR:Black, |
              FONT:regular),PAGENO
