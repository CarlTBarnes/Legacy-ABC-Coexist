EMBED %ControlEventHandling
[INSTANCES]
WHEN '?Tea:Department'
[INSTANCES]
WHEN 'AlertKey'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 2500
PROPERTY:END
!Call drag toolbox
IF NOT SelectMajorsThread
  GLO:DropThread = THREAD()
  GLO:DropControl = ?Tea:Department
  SelectMajorsThread = START(SelectMajors)
  GLO:ThreadRef &= SelectMajorsThread
END

[END]
WHEN 'Drop'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 2500
PROPERTY:END
!Receive dropped data and close drag toolbox
Tea:Department = DROPID()
DISPLAY
POST(EVENT:CloseWindow,,SelectMajorsThread)
SelectMajorsThread = 0
SELF.Reset
PRESSKEY(TabKey)
PRESSKEY(TabKey)

[END]
WHEN 'Selected'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 2500
PROPERTY:END
!Call drag toolbox
IF SELF.OriginalRequest = InsertRecord AND NOT Tea:Department
  GLO:DropThread = THREAD()
  GLO:DropControl = ?Tea:Department
  SelectMajorsThread = START(SelectMajors)
  GLO:ThreadRef &= SelectMajorsThread
END

[END]
[END]
WHEN '?Button7'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 2500
PROPERTY:END
!Call drag toolbox
POST(EVENT:AlertKey,?Tea:Department)

[END]
[END]
[END]
EMBED %WindowEventHandling
[INSTANCES]
WHEN 'CloseWindow'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Close drag toolbox
IF SelectMajorsThread
  POST(EVENT:CloseWindow,,SelectMajorsThread)
END
[END]
[END]
