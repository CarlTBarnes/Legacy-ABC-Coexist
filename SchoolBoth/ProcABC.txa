            #ORIG(?List),#SEQ(1),#ORDINAL(1)
          BUTTON('&Insert'),AT(57,158,45,14),USE(?Insert:2),HIDE,#LINK(?Change:2),#ORIG(?Insert), |
            #SEQ(2),#ORDINAL(2)
          BUTTON('&Change'),AT(106,158,45,14),USE(?Change:2),DEFAULT,HIDE,#LINK(?Delete:2), |
            #ORIG(?Change),#SEQ(2),#ORDINAL(3)
          BUTTON('&Delete'),AT(155,158,45,14),USE(?Delete:2),HIDE,#LINK(?Insert:2),#ORIG(?Delete), |
